<Window x:Class="RimWorldLauncher.Views.Main.WinMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RimWorldLauncher.Views.Main"
        mc:Ignorable="d"
        Title="MainWindow" Width="1280" Height="600">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TabControl>
            <TabItem Header="PgProfiles">
                <Grid>
                    <Frame Content="Frame" Source="/RimWorldLauncher;component/Views/Main/PgProfiles.xaml"/>
                </Grid>
            </TabItem>
            <TabItem Header="PgMods">
                <Grid>
                    <Frame Content="Frame" Source="/RimWorldLauncher;component/Views/Main/PgModpacks.xaml"/>
                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
